{
  "name": "ui",
  "version": "1.0.0-beta.3",
  "main": "index.cjs.js",
  "module": "index.js",
  "browser": "index.umd.js",
  "sideEffects": [
    "*.css"
  ],
  "repository": "git@github.com:dhis2/ui.git",
  "author": "Viktor Varland <viktor@dhis2.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "format": "code-style",
    "test": "NODE_ENV=test tape -r babel-register tests/*.js",
    "start": "node scripts/watch.js",
    "build:css": "postcss --verbose src/**/*.css --base src --dir build",
    "build:es": "babel src --out-dir build --copy-files",
    "build:files": "node scripts/setup-dist-package.js",
    "build": "NODE_ENV=production npm run build:es && npm run build:css && npm run build:files",
    "deploy": "deploy-build",
    "release": "npm run build && npm publish build"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@dhis2/code-style": "^1.0.2",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "chokidar": "^2.0.4",
    "core-js": "^2.5.7",
    "cssnano": "^4.1.3",
    "mini-css-extract-plugin": "^0.4.3",
    "nodemon": "^1.18.4",
    "postcss-cli": "^6.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.0.6",
    "postcss-rtl": "^1.3.2",
    "prop-types": "^15.6.2",
    "prop-types-exact": "^1.2.0",
    "style-loader": "^0.23.0",
    "tape": "^4.9.1"
  },
  "peerDependencies": {
    "@babel/polyfill": "^7.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "typeface-roboto": "^0.0.54"
  }
}